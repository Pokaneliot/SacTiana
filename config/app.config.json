{
  "app": {
    "name": "MonLogiciel",
    "version": "1.0.0",
    "environment": "development"
  },
  "backend": {
    "symfony": {
      "path": "../backend",
      "port": 8000,
      "url": "http://localhost:8000"
    }
  },
  "runtime": {
    "php": {
      "path": "../runtime/php",
      "version": "8.2",
      "executable": "php.exe"
    },
    "tools": {
      "path": "../runtime/tools"
    }
  },
  "storage": {
    "logs": "../storage/logs",
    "cache": "../storage/cache"
  },
  "database": {
    "type": "sqlite",
    "path": "../backend/var/data.db"
  }
}
